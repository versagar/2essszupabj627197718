* {
    box-sizing: border-box;
}

.feed::-webkit-scrollbar {
    width: 1vw;
    
}

.feed::-webkit-scrollbar-thumb {
    background-color: #c1c000;
}

.feed::-webkit-scrollbar-track {
    background-color: #632000;
}

body {
    display: block;
    margin: 0;
    padding: 0;
    overflow: hidden;
}

header {
    background-color: #6C2E00;
    width: 100vw;
    height: 8vw;
    border-bottom: 0.1vw solid #c1c6af;
    overflow: hidden;
    padding-top: 0.5vw;
    padding-bottom: 0.5vw;
}

.htitle {
    float: left;
    width: 20vw;
    height: 7vw;
    text-align: center;
    box-shadow: 0.1vw 0.1vw 0.2vw white;
    border: 0.2vw solid #8c3f06;
}

.htitle:active {
    box-shadow: none;
    border: none;
    margin-top: 0.1vw;
    margin-left: 0.1vw;
}

.h1 {
    font-size: 3.5vw;
    font-weight: bold;
    color: white;
    font-family: 'Times New Roman', Times, serif;
    text-shadow: 0.3vw 0.3vw 0.1vw black;
}

.h3 {
    font-weight: bold;
    font-size: 1.5vw;
    color: #D9D8AB00;
    text-shadow: 0.1vw 0.1vw 0.1vw white;
}

.logo {
    float: left;
    width: 7vw;
    height: 7vw;
    margin-top: 0.25vw;
}

.menu {
    float: left;
    font-size: 4vw;
    height: 7vw;
    text-align: center;
    border: 0.1vw solid #8c3f06;
    box-shadow: 0.3vw 0.3vw 0.5vw black;
    text-shadow: 0.3vw 0.3vw 0.2vw black;
}

.menu:active {
    box-shadow: none;
    border: 0.1vw solid #111;
}

.mblock {
    margin-right: 4vw;
    display: block;
    background-color: white;
    position: relative;
}

.mblock a {
    margin-right: 1vw;
    float: right;
    background-color: #C1C6AF;
    text-decoration: none;
    width: 3vw;
    border: 0.1vw solid black;
    text-align: center;
    padding: 0.1vw;
    box-shadow: 0.1vw 0.1vw 0.1vw black;
    font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif;
    color: #6C2E00;
}

.mblock a:active {
    box-shadow: none;
    margin-top: 5vw;
}

.sidenav {
    height: 100%;
    width: 0;
    position: fixed;
    z-index: 1;
    top: 0;
    left: 0;
    background-color: #111;
    overflow-x: hidden ;
    transition: 0.5s;
}

.closebtn {
    font-size: 6vw;
    margin-top: 0.5vw;
    text-decoration: none;
    float: right;
    color: #fff;
    background-color: #111;
    display: block;
}

.ml {
    padding: 1.5vw;
    text-decoration: none;
    width: 100%;
    font-size: 2vw;
    color: #f1f1f1;
    display: block;
    transition: 0.3s;
}

.ml:active {
    color: #818181;
}

.wbody {
    display: block;
    width: 100vw;
    height: calc(100vh - 16vw);
    margin: 0;
    padding: 0;
    background-color: #6C2E00;
    overflow: hidden;
}

.myFeed {
    width: 80vw;
    height: 100%;
    float: right;
    display: block;
    padding: 0;
    margin: 0;
    overflow: hidden;
}

.phead {
    width: 100%;
    font-size: 1.5vw;
    display: block;
    color: white;
    background-color: #111;
    text-align: center;
    box-shadow: 0vw 0.2vw 0vw #C1C6AF;
}

.feed {
    display: block;
    float: right;
    width: 100%;
    height: 100%;
    margin-top: 0.5vw;
    background-color: lightgrey;
    padding: 0.5vw;
    overflow: scroll;
}

#info {
    width: 100%;
    height: 100vh;
    float: right;
    display: block;
}

#wpimg {
    margin: 10px auto;
    height: auto;
    width: 30%;
    display: block;
}

.openOv {
    text-decoration: none;
    padding: 0;
    float: left;
    display: block;
    width: 0;
    text-align: center;
    margin-right: 2vw;
    color: white;
    text-shadow: 2px 2px 2px #6C2E00;
    overflow: hidden;
}

.openOv:active {
    box-shadow: -2px -2px 1px black;
    margin-top: 2px;
    margin-right: 12px;
}

.myOv {
    width: 20vw;
    height: 100%;
    float: left;
    transition: 0.3s;
    display: block;
    margin: 0;
    padding-right: 0.5vw;
    overflow: hidden;
    position: absolute;
    background-color: #6c2e00;
}

.ov {
    font-size: 1.5vw;
    color: white;
    background-color: #111;
    text-align: center;
    box-shadow: 0 0.2vw 0vw #C1C6AF;
    display: block;
    overflow: hidden;
}

.rFrame {
    width: 100% ;
    height: 15vw;
    border: 0.2vw solid #6C2E00;
    margin-top: 0.5vw;
    overflow: hidden;
}

.arframe {
    padding-top: 0.5vw;
    overflow: hidden;
    height: auto;
}

.arframe a {
    font-size: 1vw;
    width: 80%;
    height: calc(12% - 4vw);
    padding: 0.25vw;
    text-decoration: none;
    background-color: #C1C6AF;
    color: white;
    float: right;
    margin-bottom: 0.4vw;
    box-shadow: -0.3vw 0vw 0.1vw black;
    text-shadow: 0.1vw 0.1vw 0.1vw black;
    display: block;
}

.arframe a:active {
    width: 79.5%;
    box-shadow: 0.1vw 0vw 0.1vw black;
}

.foot {
    background-color: black;
    width: 100vw;
    min-height: 8vw;
    display: block;
    position: absolute;
}

.footlink {
    display: block;
    margin: 1vw auto;
    float: none;
    width: fit-content;
}

.footlink a {
    display: block;
    text-decoration: none;
    float: right;
    color: white;
    background-color: #555555;
    margin: 0.1vw;
    padding: 0.2vw;
    border: 0.1vw solid white;
    text-shadow: 0.1vw 0.1vw 0.1vw black;
    box-shadow: 0.2vw 0.2vw 0.1vw #555555;
}

.footlink a:active {
    box-shadow: none;
    margin: 0.2vw 0.1vw 0.1vw 0.2vw;
}

@media only screen and (max-width: 861px) {
    .feed {
       
    }
}
